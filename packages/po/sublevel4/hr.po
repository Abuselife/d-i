msgstr "'Nijedan' vas neće pitati nijedno pitanje."

#. Type: string
#. Description
#. :sl4:
#: ../cdebconf.templates:3001
msgid "Text"
msgstr "Tekst"

#. Type: string
#. Description
#. :sl4:
#: ../cdebconf.templates:3001
msgid "'Text' is a traditional plain text interface."
msgstr "'Tekst' je tradicionalno tekstualno sučelje."

#. Type: string
#. Description
#. :sl4:
#: ../cdebconf.templates:4001
